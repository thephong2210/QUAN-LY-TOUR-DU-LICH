@{
    ViewBag.Title = "Index";

    var jsonTemp = ViewBag.listTemp;
}
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã nhân viên
        </th>
        <th>
            Tên nhân viên
        </th>
        <th>
            Nhiệm vụ
        </th>
        <th></th>
    </tr>

@foreach (var item in jsonTemp) {
    <tr>
        <td>
            @item.maNhanVien
        </td>
        <td>
           @item.tenNhanVien
        </td>
        <td>
            @item.nhiemVu
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.maNhanVien }) |
            @Html.ActionLink("Details", "Details", new { id = item.maNhanVien }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.maNhanVien })
        </td>
    </tr>
}

</table>
